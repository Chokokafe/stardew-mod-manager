{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack" : "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "extract-zip": "^2.0.1",
    "glob": "^10.4.1",
    "helmet": "^7.1.0",
    "node-fetch": "^2.6.6",
    "selenium-webdriver": "^4.21.0",
    "strip-bom": "^5.0.0",
    "strip-json-comments": "^3.1.1"
  },
  "build": {
    "appId": "com.chokokafe.stardew-valley-mod-manager",
    "copyright": "Copyright Â© 2024 Chokokafe",
    "productName": "Stardew Valley Mod Manager",
    "electronVersion": "31.0.0",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "extraFiles": [
      "modlistdefinitive.json"
    ]
  }
}
